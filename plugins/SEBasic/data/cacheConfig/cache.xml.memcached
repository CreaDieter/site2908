<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
    <backend>
        <type>Pimcore_Cache_Backend_Memcached</type>
        <custom>true</custom>
        <options>
            <servers>
                <host>localhost</host>
                <port>11211</port>
                <persistent>true</persistent>
                <compatibility>true</compatibility>
            </servers>
        </options>
    </backend>
</zend-config>