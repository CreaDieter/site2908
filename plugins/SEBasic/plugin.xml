<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
		<plugin>

            <!-- the plugin name [A-Za-z_] -->
            <pluginName>SEBasic</pluginName>

            <!-- a short description -->
			<pluginDescription><![CDATA[Studio Emma Basic Plugin: Everything starts here]]></pluginDescription>

            <!-- meta data -->
            <pluginVersion>2.0</pluginVersion>
            <pluginRevision>2000</pluginRevision>
            <pluginBuildTimestamp>0</pluginBuildTimestamp>

            <!-- put in the URL for the plugin configuration eg. /plugin/SEBasic/admin/index -->
            <pluginIframeSrc></pluginIframeSrc>

            <!-- className of the plugin which extends Pimcore_API_Plugin_Abstract-->
			<pluginClassName>SEBasic_Plugin</pluginClassName>

            <!-- include paths relative to plugin-directory -->
			<pluginIncludePaths>
				<path>/SEBasic/lib</path>
				<path>/SEBasic/services</path>
				<path>/</path>
			</pluginIncludePaths>

			<!-- namespaces to register with autoloader-->
			<pluginNamespaces>
				<namespace>SEBasic</namespace>
			</pluginNamespaces>

			<!-- js files with path relative to plugin-directory -->
			<pluginJsPaths>
                <path>/plugins/SEBasic/static/js/startup.js</path>
			</pluginJsPaths>

			<!-- css files with path relative to plugin-directory -->
			<pluginCssPaths>
			</pluginCssPaths>
		</plugin>
</zend-config>
